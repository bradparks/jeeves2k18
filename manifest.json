{
  "manifest_version": 2,
  "name": "Jeeves 2k18",
  "description": "My absolute man, Jeeves. Personal Web Assistant ",
  "version": "1.0",
  "minimum_chrome_version": "20",
  "permissions": [
  "tabs", "activeTab"],
  "content_scripts": [
    {
      "js": ["logic.js", "jquery.js"],
      "matches": ["<all_urls>"]
    }],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
